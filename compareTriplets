function compareTriplets(a, b) {
    let arr = [0, 0];
    for (let i = 0; i < a.length; i++) {
        if (a[i] > b[i]) {
            arr[0] += 1;
        } else if (a[i] < b[i]) {
            arr[1] += 1;
        } else {
            arr[0] += 0;
        }
    }
    return arr;
}

let a = [5, 6, 7]
let b = [3, 6, 10]

console.log(compareTriplets(a, b));